<scxml xmlns="http://www.w3.org/2005/07/scxml"
       xmlns:cs="http://commons.apache.org/scxml"
       version="1.0"
       initial="start">

    <state id="start">
        <transition event="s_store_sk" target="s_store_sk"/>
    </state>

    <state id="s_store_sk">
        <onentry>
            <assign name="var__s_store_sk" expr="%sequance(s_store_sk,0,1)"/>
        </onentry>
        <transition event="s_store_id" target="s_store_id"/>
    </state>

    <state id="s_store_id">
        <onentry>
            <assign name="var__s_store_id" expr="%alpha(16)"/>
        </onentry>
        <transition event="end" target="end"/>
    </state>

    <state id="end">
        <!-- We're done -->
    </state>
</scxml>
